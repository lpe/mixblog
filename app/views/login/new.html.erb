<h1>Login</h1>

<%= form_with model: @user, url: login_index_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password %>
     <%= form.password_field :password %>
  </div>
  <div>
    <%=  form.submit "Login" %>
  </div>
<% end %>